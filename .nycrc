{
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "build",
      "**/*.d.ts",
      "test/reports"
    ],
    "reporter": [
      "text",
      "html",
      "lcov"
    ],
    "all": true,
    "cache": true,
    "check-coverage": true,
    "statements": 95,
    "functions": 95,
    "branches": 95,
    "lines": 95,
    "sourceMap": true,
    "instrument": true,
    "temp-dir": "test/reports/.nyc_tmp",
    "report-dir": "test/reports"
  }
  